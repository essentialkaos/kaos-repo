[kaos-testing]
name=ESSENTIAL KAOS Testing (Unstable) Repo for RHEL$releasever/CentOS$releasever - $basearch
baseurl=https://yum.kaos.io/$releasever/testing/$basearch
mirrorlist=file:///usr/local/share/kaos-repo/$releasever/testing/$basearch.mirrors
failovermethod=priority
priority=1
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ESSENTIALKAOS

