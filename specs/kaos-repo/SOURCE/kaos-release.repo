[kaos-release-src]
name=ESSENTIAL KAOS Release (Stable) Repo for EL$releasever - $basearch
baseurl=https://yum.kaos.st/$releasever/release/SRPMS
priority=1
enabled=0
gpgcheck=1
module_hotfixes=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ESSENTIALKAOS
bugtracker_url=https://github.com/essentialkaos/kaos-repo/issues
