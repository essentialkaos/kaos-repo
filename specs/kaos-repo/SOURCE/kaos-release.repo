[kaos-release]
name=ESSENTIAL KAOS Release (Stable) Repo for EL$releasever - $basearch
baseurl=https://yum.kaos.st/$releasever/release/$basearch
priority=1
enabled=1
gpgcheck=1
module_hotfixes=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ESSENTIALKAOS
