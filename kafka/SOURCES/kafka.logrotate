"/var/log/kafka/*.log.????-??-??-??" {
       su kafka kafka
       daily
       rotate 90
       compress
       notifempty
       missingok
       nocreate
}

/var/log/kafka/*-gc.log {
       su kafka kafka
       daily
       rotate 90
       copytruncate
       delaycompress
       compress
       notifempty
       missingok
}

