# Bibop recipe for id3lib

# See more: https://kaos.sh/bibop
# Recipe generated by bop (https://kaos.sh/bop)

pkg id3lib id3lib-devel

fast-finish yes

command "-" "Check apps"
  app id3convert
  app id3cp
  app id3info
  app id3tag

command "-" "Check shared libs"
  lib-loaded libid3-3.8.so.*

command "-" "Check headers"
  lib-header id3
  lib-header id3.h

