# Bibop recipe for hdf
# See more: https://kaos.sh/bibop

pkg hdf hdf-devel

fast-finish yes

command "-" "Check apps"
  app gif2hdf
  app h4cc
  app h4fc
  app h4redeploy
  app hdf24to8
  app hdf2gif
  app hdf2jpeg
  app hdf8to24
  app hdfcomp
  app hdfed
  app hdfimport
  app hdfls
  app hdfpack
  app hdftopal
  app hdftor8
  app hdfunpac
  app hdiff
  app hdp
  app hncdump
  app hncgen
  app hrepack
  app jpeg2hdf
  app paltohdf
  app r8tohdf
  app ristosds
  app vmake
  app vshow

command "-" "Check headers"
  lib-header hdf
